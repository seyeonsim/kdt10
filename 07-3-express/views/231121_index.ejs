<body>
<%- include('./component/231121_header')%>
    <h1>Hello EJS!</h1>
    <% for (let i = 0; i < 5 ; i++) { %>
        <div>hi~ <%= i %>번째</div>
        <%= i %>
    <% } %>

    <!-- 231122 수업 -->
    <!-- btns 버튼 -->
    <% console.log(btns) %>
