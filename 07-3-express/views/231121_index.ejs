        <button type="button" class="btn btn-info"><%= btn %></button>
    <% } %>

    <!-- isLogin -->
    <% if(!isLogin) {  %>
        <h4>아직 로그인을 안 했군요!</h4>
        <% } else { %>
            <h4>로그인 완료!</h4>
            <div><%= me.name %></div>
            <div><%= me.msg %></div>
            <% } %>

